+#define AR1335_DEFAULT_FPS	120
+
+#define AR1335_DEFAULT_DATAFMT	MEDIA_BUS_FMT_YUYV8_2X8
+#define AR1335_DEFAULT_COLORSPACE V4L2_COLORSPACE_SRGB
